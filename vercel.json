{
  "functions": {
    "apps/web/src/app/api/*/route.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}